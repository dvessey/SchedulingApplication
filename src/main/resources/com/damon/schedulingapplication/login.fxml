<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.damon.schedulingapplication.Controller.LoginController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label id="loginLabel" fx:id="loginLabel" layoutX="261.0" layoutY="95.0" text="Login">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Label fx:id="usernameLabel" layoutX="14.0" layoutY="192.0" text="Username" />
            <TextField fx:id="usernameTextField" layoutX="111.0" layoutY="188.0" />
            <Label fx:id="passwordLabel" layoutX="270.0" layoutY="192.0" text="Password" />
            <PasswordField fx:id="passwordField" layoutX="358.0" layoutY="188.0" />
            <Label fx:id="userLocationLabel" layoutX="335.0" layoutY="36.0" text="Location" />
            <TextField fx:id="userLocationField" disable="true" layoutX="433.0" layoutY="32.0" />
            <Button fx:id="logInButton" layoutX="407.0" layoutY="313.0" mnemonicParsing="false" onAction="#onLogIn" text="Login" />
            <Label fx:id="emptyLabel" layoutX="220.0" layoutY="234.0" text="empty" textFill="RED" visible="false" />
            <Label fx:id="errorLabel" layoutX="282.0" layoutY="149.0" text="error" textFill="RED" visible="false" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
